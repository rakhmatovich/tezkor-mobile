{
  "extends": "@react-native/typescript-config/tsconfig.json",
  "compilerOptions": {
    "baseUrl": "./",
    "paths": {
      "@core/*": [
        "./src/core/*"
      ],
      "@home/*": [
        "./src/home/*"
      ],
      "@user/*": [
        "./src/user/*"
      ],
      "@category/*": [
        "./src/category/*"
      ],
      "@cart/*": [
        "./src/cart/*"
      ]
    },
    "typeRoots": [
      "types/"
    ]
  },
}
